export type SurvivorStateType = "idle" | "work" | "eat" | "sleep" | "heal" | "dead" | "follow";

export default class SurvivorStateInfo {
    /* 宿舍ID(明确关系) */
    public dormId: number;
    /** 生存者所属建筑id */
    public buildId: number;
    /** 生存者的唯一id */
    public uniqueId: number;
    /** 生存者饱腹度 */
    public satiety: number;
    /** 生存者疲劳度 */
    public fatigue: number;
    /** 生存者疾病度 */
    public illness: number;
    /** 生存者状态 */
    public state: SurvivorStateType;
}
